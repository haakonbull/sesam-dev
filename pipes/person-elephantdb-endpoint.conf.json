{
  "_id": "person-elephantdb-endpoint",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "person-elephantdb"
  },
  "sink": {
    "type": "sql",
    "system": "elephantdb",
    "create_table_if_missing": true,
    "schema_definition": [{
      "type": "string",
      "name": "address",
      "max_size": 1234,
      "min_size": 0,
      "primary_key": false,
      "source_property": "address"
    }, {
      "type": "string",
      "name": "firstname",
      "max_size": 1234,
      "min_size": 0,
      "source_property": "firstname"
    }, {
      "type": "string",
      "name": "lastname",
      "max_size": 1234,
      "min_size": 0,
      "source_property": "lastname"
    }, {
      "type": "string",
      "name": "id",
      "max_size": 1234,
      "min_size": 0,
      "primary_key": true,
      "source_property": "_id"
    }],
    "table": "person"
  },
  "pump": {
    "mode": "$ENV(pump-mode)"
  }
}
