{
  "_id": "namespace-labs",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "global-person"
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "_id"],
        ["copy", "hr-person:StreetAddress"],
        ["add", "erp-address", "_S.erp-person:StreetAddress"],
        ["add", "crm-adress", "_S.crm-person:Address"]
      ]
    }
  }
}
