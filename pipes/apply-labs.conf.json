{
  "_id": "apply-labs",
  "type": "pipe",
  "source": {
    "type": "embedded",
    "entities": {
      "_id": "1",
      "list": ["some string", false, "another string", 45678908765, "abcd", "STRING", true, null, "132456"]
    }
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"],
        ["merge-union",
          ["apply", "type-check", "_S.list"]
        ]
      ],
      "type-check": [
        ["if",
          ["is-string", "_S."],
          ["add", "result", "_S."]
        ]
      ]
    }
  },
  "comment": "halla"
}
